@model IEnumerable<CrystalBallpro.Models.Availability>

@{
    ViewBag.Title = "ScheduleCreate";
}

<h2>Schedule</h2>

<div>
    @Html.ActionLink("View Availability", "ScheduleIndex", "Admins")
</div>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Admin.Name)
